			if (stmt.execute(query)) {
				ResultSet rs = stmt.getResultSet();
				pkValue = rs.getString(1);
				Log.msg(&quot;Execute: &quot; + query + &quot;, Primary Key: &quot; + pkName + &quot; = &quot; + pkValue);
