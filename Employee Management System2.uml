            <ownedParameter xmi:id="_Z3UN-6P_EeS6jbVv0VFO_w" name="employeeId">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            </ownedParameter>
            <language>java</language>
            <body>String where = &quot;`employee_id` = '&quot; + employeeId + &quot;' ORDER BY `timestamp` DESC&quot;;
		ArrayList&lt;Model> commentList = commentModel.getDataList(where);
		employeeEditView.populateCommentList(commentList);
