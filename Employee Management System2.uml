            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_Z3mh4aP_EeS6jbVv0VFO_w" type="_Z0EstaP_EeS6jbVv0VFO_w" direction="return"/>
          <language>java</language>
          <body>ArrayList&lt;Model> rows = new ArrayList&lt;>();
		String query;
		if (where != null) {
			query = sql.select().from(tblName).where(where).getQuery();
		} else {
			query = sql.select().from(tblName).getQuery();
		}
		try (Statement stmt = this.conn.createStatement()) {
			if (stmt.execute(query)) {
				ResultSet rs = stmt.getResultSet();
				int colCount = rs.getMetaData().getColumnCount();
				int rowCount = 0;
				while (rs.next()) {
					HashMap&lt;String, String> row = new HashMap&lt;>();
					for (int i = 1; i &lt;= colCount; i++) {
						row.put(rs.getMetaData().getColumnName(i), rs.getString(i));
					}
					Model model = new Model(conn, tblName, pkName);
					model.setData(row);
					rows.add(model);
					rowCount++;
				}
				Log.msg(&quot;Execute: &quot; + query + &quot;, Rows: &quot; + rowCount);
			}
		} catch (SQLException e) {
			Log.msg(e.getMessage());
		}
		return rows;
	</body>
        </ownedBehavior>
